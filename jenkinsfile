		agent any
		
		stages {
			
			stages("build"){
				
				steps {
					echo 'building the application'
				}
			}
			stages("test"){
				
				steps {
					echo 'testing the application'
				}
			}
			stages("deploy"){
				
				steps {
					acho 'deploying the application'
				}
			}
		}
	
	}
